# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/abandoned_by_me

# PostgreSQL Settings (for docker-compose)
POSTGRES_DB=abandoned_by_me
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_PORT=5432

# GitHub OAuth Configuration
CLIENT_CODE=your_github_client_id_here
CLIENT_SECRET=your_github_client_secret_here

# Public Environment Variables (accessible in browser)
PUBLIC_CLIENT_CODE=your_github_client_id_here

# Security
JWT_SECRET=your-jwt-secret-key-here
COOKIE_SECRET=your-cookie-secret-key-here

# Development Configuration
DEBUG=false
NODE_ENV=development

# Application
APP_PORT=3000